{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
		"rules": {
			"import/no-unresolved": 0,
			"import/extensions": 0,
      "no-console": 1,
      "no-unused-expressions": 0,
      "prettier/prettier": [2, { "singleQuote": true }]
	 }
}
